{
    "plugins" : [
        "googleappsscript"
    ],
    "env": {
        "browser": true,
        "googleappsscript/googleappsscript": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            2,
            "never"
        ]
    }
}
